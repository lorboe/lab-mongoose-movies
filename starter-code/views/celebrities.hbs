<h2>Celebrities</h2>
<ul>
{{#each listOfCelebs}}
<li>
  <a href="/celebrities/{{this._id}}">
      {{this.name}}
  </a>
  <form action="celebrities/{{this._id}}/delete" method="post">
    <button type="submit">Delete</button>
  </form>
</li>
{{/each}}
</ul>

<a href="/new-celeb">Add Celebrity</a>


 